 
<h2 class="title"> </h2>
<div class="container">

<p id="timestamp" style="display: none"><%=@values[:timestamp].to_json.html_safe %></p>
<p id="significantwaveheight" style="display: none"><%=@values[:significantwaveheight].to_json.html_safe %></p>

<h2>ALTURA DE ONDAS</h2>
<canvas id="myChart0" width="400" height="200"></canvas>
<script>
var ctx = document.getElementById("myChart0").getContext('2d');
var myChart = new Chart(ctx, {
    type: 'line',
    data: {
        labels: <%= @values[:timestamp].to_json.html_safe %>,
        datasets: [{
            label: '# of Votes',
            data: <%= @values[:significantwaveheight].to_json.html_safe %>,
            borderColor: [
                'rgba(255,99,132,1)',
                'rgba(54, 162, 235, 1)',
                'rgba(255, 206, 86, 1)',
                'rgba(75, 192, 192, 1)',
                'rgba(153, 102, 255, 1)',
                'rgba(255, 159, 64, 1)'
            ],
            borderWidth: 1
        }]
    },
    options: {
        scales: {
            yAxes: [{
                ticks: {
                    beginAtZero:false
                }
            }]
        }
    }
});
</script>

